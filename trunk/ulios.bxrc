romimage: file=$BXSHARE/BIOS-bochs-latest, address=0xE0000
megs: 32
vgaromimage: file=$BXSHARE/VGABIOS-lgpl-latest
vga: extension=vbe
ata0: enabled=1, ioaddr1=0x1f0, ioaddr2=0x3f0, irq=14
ata0-master: type=disk, mode=flat, translation=auto, path="E:\My Virtual Machines\ulios\ulios-flat.vmdk", cylinders=207, heads=16, spt=63, biosdetect=auto, model="Generic 1234"
boot: disk
log: bochsout.txt
mouse: enabled=0
keyboard_mapping: enabled=1, map=$BXSHARE/keymaps/x11-pc-us.map
